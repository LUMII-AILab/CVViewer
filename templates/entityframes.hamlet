<h1>#{name} <small>(#{nr})</small>

$forall (section_frames, title) <- sections
    $if not $ null section_frames
        <h3>#{title}
        <ul>
          $forall (Frame id description frametype sentenceID source document frametext elements) <- section_frames
            <li><span data-toggle="popover" title="#{frametype}, ID: #{id}" data-content="Avots: #{source}<br/>Dokuments: #{document}<br/>Teikuma ID: #{sentenceID}<br/>#{describeElements elements}<br/>#{description}"><a href="http://www.leta.lv/press_releases/#{document}">#{frametext}</a></span> <span class="glyphicon glyphicon-ok"></span> <span class="glyphicon glyphicon-remove"></span>
